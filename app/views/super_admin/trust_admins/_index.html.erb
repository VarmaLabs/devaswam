
<%# Displays list of @trust_admins %>
<div id="div_trust_admin_collections">
	<% @trust_admins.each do |trust_admin| %>
		<div id="div_trust_admin_<%= trust_admin.id %>">
			<%= render :partial=>"item", :locals=>{:trust_admin=>trust_admin} %>
		</div>
	<% end %>
	<% if @trust_admins.blank? %>
		<div id="div_trust_admin_empty" class="box text-color-grey p-80 text-align-center" style="height:200px;">
      <%= translate("forms.no_results_found") %>
		</div>
	<% end %>
</div>
<div class="cl"></div>

<% if @trust_admins.any? %>
   <%= content_tag :div, :class=>"pull-right" do %>
     <%= paginate @trust_admins, :remote => true %>
   <% end %>
<% end %>
<div class="cl"></div>
