
<%# Displays list of @admins %>
<div id="div_admin_collections">
	<% @admins.each do |admin| %>
		<div id="div_admin_<%= admin.id %>">
			<%= render :partial=>"item", :locals=>{:admin=>admin} %>
		</div>
	<% end %>
	<% if @admins.blank? %>
		<div id="div_admin_empty" class="box text-color-grey p-80 text-align-center" style="height:200px;">
      <%= translate("forms.no_results_found") %>
		</div>
	<% end %>
</div>
<div class="cl"></div>

<% if @admins.any? %>
   <%= content_tag :div, :class=>"pull-right" do %>
     <%= paginate @admins, :remote => true %>
   <% end %>
<% end %>
<div class="cl"></div>
