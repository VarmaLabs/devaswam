<div class="btn-group mt-10 mb-10" style="width:100%;">
  <button type="button" class="btn btn-default btn-block dropdown-toggle" data-toggle="dropdown">
    Choose a Admin <span class="caret"></span>
  </button>
  <ul class="dropdown-menu" role="menu">
    <% @admins.each do |admin| %>
      <li><%= link_to admin.name, super_admin_admin_path(admin), :remote=>true %></li>
    <% end %>
    <%= content_tag :li, link_to_next_page(@admins, 'Next Page', :remote => true) %>
    <%= content_tag :li, link_to_previous_page(@admins, 'Previous Page', :remote => true) %>
  </ul>
</div>