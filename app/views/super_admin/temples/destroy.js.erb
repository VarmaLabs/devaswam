<% if @temple %>
  <% if @temple.new_record? %>

  	// Fill the right box with first temple details
  	$('#div_temple_details').html("<%= escape_javascript(render(:partial=>'summary')) %>");

  <% else %>

  	// Render the show partial to show flash message
  	$('#div_temple_details').html("<%= escape_javascript(render(:partial=>'show')) %>");

  <% end %>

  // Render temples List
  $('#div_temple_collections').html("<%= escape_javascript(render(:partial=>'index')) %>");

<% else %>

  var noResultsText = "<div id=\"div_admin_empty\" class=\"box text-color-grey p-80 text-align-center\" style=\"height:200px;\"><%= translate("forms.no_results_found") %></div>";
  $('#div_temple_index').html(noResultsText);
  $('#div_temple_details').html(noResultsText);

<% end %>
